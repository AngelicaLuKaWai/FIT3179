{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/AngelicaLuKaWai/FIT3179/main/world_population.csv"
  },
  "params": [{"name": "grid", "select": "interval", "bind": "scales"}],
  "mark": "circle",
  "encoding": {
    "y": {"field": "2022 Population", "type": "quantitative"},
    "x": {
      "field": "Growth Rate",
      "type": "quantitative",
      "scale": {"domain": [0.9, 1.1]}
    },
    "color": {"field": "Country", "type": "nominal"},
    "size": {
      "field": "Growth Rate",
      "type": "quantitative",
      "scale": {"domain": [0.9, 1.1]}
    },
    "tooltip": [
      {"field": "Country", "type": "nominal"},
      {"field": "Continent", "type": "nominal", "title": "Continent"},
      {"field": "2022 Population", "type": "quantitative"},
      {"field": "Growth Rate", "type": "quantitative"}
    ]
  },
  "config": {}
}
